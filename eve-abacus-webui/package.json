{
  "name": "eve-abacus-webui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "node -e \"const fs=require('fs');const path=require('path');if(fs.existsSync('.next/standalone')){const staticSrc='.next/static';const staticDest='.next/standalone/.next/static';if(fs.existsSync(staticSrc)){if(!fs.existsSync('.next/standalone/.next'))fs.mkdirSync('.next/standalone/.next',{recursive:true});require('child_process').execSync('cp -r '+staticSrc+' '+staticDest,{stdio:'inherit'});}}\"",
    "build:with-types": "pnpm run generate-types && next build",
    "start": "next start",
    "lint": "next lint",
    "generate-types": "openapi-typescript http://localhost:5000/swagger/v1/swagger.json -o src/types/generated.ts --prettier",
    "generate-types:dev": "openapi-typescript http://localhost:5000/swagger/v1/swagger.json -o src/types/generated.ts --prettier --watch"
  },
  "dependencies": {
    "critters": "^0.0.25",
    "ioredis": "^5.3.2",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redis": "^4.6.13",
    "styled-jsx": "^5.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-jsdoc": "^6.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "openapi-typescript": "^7.8.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
